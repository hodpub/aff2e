<section class='tab magic scrollable {{tab.cssClass}}' data-group='primary' data-tab='magic'>
  {{#each spells as | spellList spellType |}}
  {{#if spellList}}
  <div class="item-title">
    <label>{{localize (concat "AFF.Actor.base.magic." spellType)}}</label>
  </div>
  <div class="list grid">
    {{#each spellList as | item |}}
    {{log (lookup item "template")}}
    {{> (lookup item "template") item=item}}
    {{/each}}
  </div>
  {{log @last}}
  {{#unless @last}}
  <div class="horizontal-rule"></div>
  {{/unless}}
  {{/if}}
  {{/each}}
</section>